{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "package": "electron-packager . ElectronStart --out=./packages --version=1.4.3 --overwrite",
    "build": "build"
  },
  "build": {
    "appId": "com.nova-controls.garykeeble",
    "mac": {
      "category": "public.app-category.hobby"
    },
    "win": {
      "iconUrl": "(windows-only) https link to icon"
    }
  },
  "devDependencies": {
    "electron-builder": "next",
    "electron": "1.4.3"
  },
  "repository": "https://nova-controls.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0"
}
